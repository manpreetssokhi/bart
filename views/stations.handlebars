<div class="container">
    <h1 class="display-4 pt-3">Stations List</h1>
    <div class="row">
        <div class="col-md-6">
            <ul>
                {{#each context}}
                {{!-- <li><a href="stations/{{this.abbr}}">{{this.name}}</a></li> --}}
                <li>
                    <span style="color: #1FA2FF"> {{ this.name }} </span> 
                    <span style="color: #E3000B"> {{ this.abbr }} </span>
                </li>
                {{/each}}
            </ul>
        </div>
    </div>
</div>